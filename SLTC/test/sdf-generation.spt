module sdf-generation

language SLTC

setup Common

// FXIME: How to ignore the analysis result for tests?
test empty-module [[
  module test
  imports test
  language test
  meta-variables
  contexts
  judgments
  rules
  conjectures
]] run toSdf to  module("Generated"
        			   , [imports("test"), imports("Common")]
        			   , exports(conc-grammars(context-free-syntax([]),
