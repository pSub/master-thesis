module constraint-language

signature
    sorts Term Constraint Constraints
    constructors
       Var__ : String -> Term
       Term : String * List(Term) -> Constraint              // Constructor x Parameters
       Eq__ : Term * Term -> Constraint
       Constraints__ : List(Constraint) -> Constraints
       
rules
    makeVar : v -> Var__(v)
    makeEq : (a,b) -> Eq__(a,b)