module utils

imports libstratego-lib
	
rules
	intersperse (|s) : [] -> []
    intersperse (|s) : [x] -> [x]
    intersperse (|s) : [x|y] -> [x,s| <intersperse(|s)> y]

    option-to-string : Some(s) -> s
    option-to-string : None()  -> ""
    
    newnames(|l) : 0 -> []
    newnames(|l) : n -> [<newname> l | <newnames(|l)> <subt> (n, 1)]
    
    fst : (a, b) -> a
    snd : (a, b) -> b