module tests-generate-constraints

imports generate-constraints

rules
    
    
    search-tests = apply-test(!"search", search(|[]),
                              !"x", ![]);
                   apply-test(!"search", search(|["x"]),
                              !"x",  ![("x", [1])]);
                   apply-test(!"search", search(|["a", "x", "b", ["x", "y", "z"]]),
                              !"x",
                              ![("x", [2]), ("x", [4, 1])])
    
    tests-generate-constraints =
        search-tests