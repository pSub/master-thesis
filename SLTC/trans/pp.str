module pp

imports

	libstratego-gpp
	lib/runtime/tmpl/pp
	Common.pp.generated
	SLTC.pp.generated

rules
	
  pp-SLTC-string =
	 !V([], <id>)
    ; box2text-string(|120)
      
  pp-partial-SLTC-string =
    prettyprint-example
    ; !V([], <id>)
    ; box2text-string(|120)
      
  pp-debug :
    ast -> result
    with
       result := <pp-SLTC-string> ast
    <+ <bottomup(try(not(is-string); not(is-list); not(pp-SLTC-string); debug(!"cannot pp ")))> ast
    ;  result := ""

